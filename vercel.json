{
  "builds": [
    {
      "src": "./api/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/transribe",
      "methods": ["POST"],
      "dest": "/index.js"
    },
    {
      "src": "/completions",
      "headers": { "Content-Type": "application/json" },
      "methods": ["POST"],
      "dest": "/index.js"
    },
    {
      "src": "/eleven",
      "headers": { "Content-Type": "application/json" },
      "methods": ["POST"],
      "dest": "/index.js"
    }
  ]
}
